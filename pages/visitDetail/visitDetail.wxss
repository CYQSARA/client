page{
background:#f3f3f3;
}
.flex
{
  display: flex;
  width:300px;
  height: 90px;
}
.item{
  padding: 10px;
  background-color: #fff;
  color: #787878;
  border-bottom: 1px solid  #CDCCD0;   /**下横线**/
}
.item image
{
  display: block;
  overflow: hidden;
  flex:1;
}
.item-middle
{
  flex:0;
  margin-left:10px;
  padding-top: 5px;
  flex-grow:4;
}
.title
{
  font-size: 15px;
  color:gray;
  text-align:left;
}
.sub-title
{
  font-size: 12px;
  color:gray;
  margin: 2px;
}
.item-right
{
  height:140px;
  width:100px;
  margin-top: 10px;
  display: inline-block;
  padding: 2px 4px;
  font-size: 14px;
  line-height: 16px;
}
.block
{
  background:#fff;
  margin-top:15rpx;
  height:70px;
}
.countStyle
{
   font-size:15px;
   display:inline-block;
   width:33%;
   text-align:center;
   margin-top:15px;
}
.lineBorder
{
   border-right: 0.2px solid  #CDCCD0;
}
.pro-block
{
  background:#fff;
  margin-top:15rpx;
  height:160px;
}

.uploadWrap{height:120px; width:100%; display: flex; display: -webkit-box; flex-direction: column;}
.upload_Item{ width: 120px; height: 120px; flex: 1;}
.upload_Item_img{ width: 100px; height: 80px;float:left;}
.pro-bg
{
  margin-top: 15rpx;
  background:white;
  height:80px;
  width:100%;
}
.common-text
{
  font-size:15px;
  margin-left:10px;
  flex:1;
}
.borderBottom
{
  margin-top:5px;
  border-bottom: 0.2px solid  #CDCCD0;   /**下横线**/
}
.radio-group {
}
.radio {
  width:40%;
  margin: 5px;
  float: left;
  padding-top: 5px;
}
.radio_1 {
  width:30%;
  margin: 5px;
  float: left;
  padding-top: 5px;
}